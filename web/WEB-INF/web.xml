<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>tmsModelLayer.ServletPackage.ServletRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>tmsModelLayer.ServletPackage.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Client_order</servlet-name>
        <servlet-class>tmsModelLayer.ServletPackage.Client_orderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProviderActionServlet</servlet-name>
        <servlet-class>tmsModelLayer.ServletPackage.ProviderActionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Client_order</servlet-name>
        <url-pattern>/Client_order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProviderActionServlet</servlet-name>
        <url-pattern>/ProviderActionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
